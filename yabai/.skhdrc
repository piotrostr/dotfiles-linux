# open terminal
cmd - return : /Applications/Alacritty.app/Contents/MacOS/alacritty

# focus window
cmd - h : yabai -m window --focus west 
cmd - j : yabai -m window --focus south
cmd - k : yabai -m window --focus north
cmd - l : yabai -m window --focus east 

# alt - h : yabai -m space --focus south
# alt - j : yabai -m space --focus north
# alt - k : yabai -m space --focus east
# alt - l : yabai -m space --focus west

0x0A : skhd -k "tab"

# move window, change desktop if no window
shift + cmd - h : yabai -m window --warp west 
shift + cmd - j : yabai -m window --warp sout 
shift + cmd - k : yabai -m window --warp nort 
shift + cmd - l : yabai -m window --warp east 
# alt + cmd - h : yabai -m display --focus west

# create desktop and move focus
shift + cmd - n : yabai -m space --create; \
                  id=$(yabai -m query --displays | jq '.[]' | jq '.spaces' | jq '.[-1]'); \
                  yabai -m window --space $id
                  # yabai -m space --focus $id \  # do that if also want to move focus
                  
                  
# move window to given desktop and move focus
alt - 1 : yabai -m window --space 1
alt - 2 : yabai -m window --space 2
alt - 3 : yabai -m window --space 3
alt - 4 : yabai -m window --space 4
alt - 5 : yabai -m window --space 5

# delete current desktop
shift + cmd - 0x29 : yabai -m space --destroy

# go to desktop
f1 : yabai -m space --focus 1
f2 : yabai -m space --focus 2
f3 : yabai -m space --focus 3
f4 : yabai -m space --focus 4
f5 : yabai -m space --focus 5
f6 : yabai -m space --focus 6

# rotate 
shift + cmd - r: yabai -m space --rotate 90

