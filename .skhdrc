# open terminal
cmd - return : /Applications/kitty.app/Contents/MacOS/kitty --single-instance -d ~

# focus window
cmd - h : yabai -m window --focus west 
cmd - j : yabai -m window --focus south
cmd - k : yabai -m window --focus north
cmd - l : yabai -m window --focus east

# move window
shift + cmd - h : yabai -m window --warp west
shift + cmd - j : yabai -m window --warp south
shift + cmd - k : yabai -m window --warp north
shift + cmd - l : yabai -m window --warp east

# create desktop and move focus
shift + cmd - n : yabai -m space --create; \
                  id=$(yabai -m query --displays | jq '.[]' | jq '.spaces' | jq '.[-1]'); \
                  yabai -m space --focus $id


# delete current desktop
shift + cmd - 0x29 : yabai -m space --destroy

# go to desktop
f1 : yabai -m space --focus 1
f2 : yabai -m space --focus 2
f3 : yabai -m space --focus 3
f4 : yabai -m space --focus 4
f5 : yabai -m space --focus 5
f6 : yabai -m space --focus 6

