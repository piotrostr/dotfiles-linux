{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "json",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "graphql",
    "solidity",
    "python",
    "go",
    "rust",
    "cpp",
    "cmake"
  ],
  "coc.preferences.enableFloatHighlight": true,
  "prettier.formatterPriority": 4,
  "languageserver": {
    "solidity": {
      "command": "/usr/bin/solc",
      "args": ["--lsp"],
      "trace.server": "verbose",
      "rootPatterns": [".git/"],
      "filetypes": ["solidity"]
    },
    "efm": {
      "command": "efm-langserver",
      "filetypes": ["vim", "eruby", "markdown", "yaml"]
    },
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    },
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "objc", "objcpp"],
      "rootPatterns": [
        ".ccls",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
      }
    }
  },
  "git.enableGlobalStatus": false,
  "tsserver.enable": true,
  "tsserver.enableJavascript": true,
  "tsserver.enableTracing": true,
  "python.formatting.provider": "black",
  "pyright.enable": true,
  "python.analysis.autoSearchPaths": true,
  "python.analysis.useLibraryCodeForTypes": true,
  "go.enable": true,
  "go.trace.server": "verbose",
  "go.goplsUseDaemon": true,
  "go.checkForUpdates": "install",
  "lua.enable": true,
  "typescript.suggest.classMemberSnippets.enabled": true,
  "suggest.fixInsertedWord": true,
  "suggest.removeDuplicateItems": false,
  "suggest.triggerCompletionWait": 30,
  "suggest.maxCompleteItemCount": 100,
  "suggest.completionItemKindLabels": {
    "method": "  ",
    "function": "  ",
    "variable": "  ",
    "field": "  ",
    "typeParameter": "  ",
    "constant": "  ",
    "class": " פּ ",
    "interface": "  ",
    "struct": "  ",
    "event": "  ",
    "operator": "  ",
    "module": "  ",
    "property": "  ",
    "enum": " 練",
    "reference": "  ",
    "keyword": "  ",
    "file": "  ",
    "folder": " ﱮ ",
    "color": "  ",
    "unit": " 塞 ",
    "snippet": "  ",
    "text": "  ",
    "constructor": "  ",
    "value": "  ",
    "enumMember": "  "
  },
  "snippets.loadFromExtensions": true,
  "suggest.languageSourcePriority": 99,
  "go.goplsOptions": {
    "gofumpt": true,
    "completeUnimported": true
  },
  "highlight.colorNames.enable": false,
  "docker.enable": true,
  "python.linting.pytypeEnabled": false,
  "python.analysis.typeCheckingMode": "off"
}
